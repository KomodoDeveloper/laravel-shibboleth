{
  "name": "clemsonmartech/laravel-shibboleth",
  "description": "Fork of clemsonmartech/laravel-shibboleth to support modern Laravel (laravel 11 and 12)",
  "authors": [
    {
      "name": "Brandon Simmons",
      "email": "bsimmo4@clemson.edu"
    }
  ],
  "minimum-stability": "stable",
  "require": {
	"illuminate/support": "^9.0 || ^10 || ^11.0 || ^12.0",
	"laravel/framework": "^9.0 || ^10 || ^11.0 || ^12.0",
    "mrclay/shibalike": "1.0.0",
    "php-open-source-saver/jwt-auth": "^1.4 || ^2.0 || ^2.8"
  },
  "autoload": {
    "psr-4": {
      "Jhu\\Wse\\LaravelShibboleth\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jhu\\Wse\\LaravelShibboleth\\ShibbolethServiceProvider"
      ]
    }
  },
  "replace": {
    "uabookstores/laravel-shibboleth": "self.version"
  },
  "provide": {
    "uabookstores/laravel-shibboleth": "self.version"
  }
}
